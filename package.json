    "@react-navigation/stack": "^5.9.3",
    "axios": "^0.20.0",
    "axios-cache-adapter": "^2.5.0",
    "crypto-js": "^3.1.9-1",
    "enzyme-to-json": "^3.6.1",
    "expo": "~37.0.3",
    "expo-asset": "~8.1.5",
    "expo-splash-screen": "^0.2.3",
    "expo-updates": "~0.2.0",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "~0.61.5",
    "react-native-fast-image": "^8.3.2",
    "react-native-fix-image": "^2.0.1",
    "react-native-flash-message": "^0.1.16",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-pan-zoom": "^2.1.12",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-iphone-x-helper": "^1.3.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-masonry-layout": "^1.0.6",
    "react-native-material-color": "^1.0.15",
    "react-native-modal": "^11.5.6",
    "react-native-modalize": "^2.0.6",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-share": "^4.0.2",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-unimodules": "^0.10.1",
    "react-native-vector-icons": "^7.1.0",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.4.2",
    "react-navigation-tabs": "^2.9.2",
    "rn-fetch-blob": "^0.11.2",
    "use-force-update": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.6.2",
    "@testing-library/jest-native": "3.1.0",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "^26.0.14",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.61.17",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-navigation": "^3.0.8",
    "@types/react-redux": "^7.1.0",
    "@types/react-test-renderer": "^16.8.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-expo": "~8.1.0",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.8.1",
    "fetch-mock": "^7.3.3",
    "husky": "3.1.0",
    "jest": "~24.9.0",
    "jest-expo": "~37.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "0.4.2",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "patch-package": "^6.1.2",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react-dom": "^16.8.6",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "16.8.6",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": false,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "babel-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>",
      "src"
    ],
    "setupFiles": [
      "./__tests__/setup.ts",
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)",
      "node_modules/(?!(jest-)?react-native|react-navigation|react-navigation-redux-helpers|@react-navigation/.*)"
    ]
  }
}
