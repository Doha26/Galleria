{
  "private": true,
  "name": "Galleria",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "postinstall": "npx jetifier && npx react-native-fix-image && grep -rl \"s.dependency 'React/Core'\" node_modules/ | xargs sed -i '' 's=React/Core=React-Core=g'",
    "android-shake": "adb shell input keyevent 82",
    "test": "jest --updateSnapshot --no-cache",
    "lint": "eslint src --ext .js,.ts,.tsx --no-ignore --max-warnings=0 && yarn ts:check"
  },
  "dependencies": {
    "@react-native-community/cameraroll": "^4.0.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "react-native-community/react-native-netinfo",
